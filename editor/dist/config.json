{
  "palette": {
    "connectors": [
      {
        "title": "default",
        "items": [
          {
            "id": "default",
            "title": "default",
            "schemaRef": "#/$defs/textOnly",
            "previewImageRef": "arrow_right"
          },
          {
            "id": "no_arrows",
            "title": "No Arrows",
            "schemaRef": "#/$defs/textOnly",
            "previewImageRef": "minus"
          }
        ]
      }
    ],
    "stencils": [
      {
        "title": "Examples",
        "items": [
          {
            "id": "white_rect",
            "title": "White Rectangle",
            "shape": {
              "shapeType": "roundrectangle",
              "label": {
                "text": "White",
                "textColor": "#343434",
                "fontSize": "12",
                "fontFamily": "Dialog"
              }
            },
            "previewImageRef": "whiteRectImage",
            "schemaRef": "#/$defs/node"
          },
          {
            "id": "smile",
            "title": "A Smile",
            "imageDataRef": "smile",
            "previewImageRef": "smile",
            "schemaRef": "#/$defs/textOnly"
          },
          {
            "id": "sign",
            "title": "A Japanese Sign",
            "imageDataRef": "sign",
            "previewImageRef": "sign"
          }
        ]
      }
    ]
  },
  "schema": {
    "$id": "https://example.net/root.json",
    "$schema": "http://json-schema.org/draft/2019-09/schema#",
    "$defs": {
      "node": {
        "type": "object",
        "properties": {
          "text": {
            "title": "Some Text",
            "type": "string",
            "extension": {
              "highlight": "markdown"
            }
          },
          "someInt": {
            "title": "Some Integer",
            "type": "integer"
          },
          "someBool": {
            "title": "Some Boolean",
            "type": "boolean"
          },
          "someFloat": {
            "title": "Some Float",
            "type": "number"
          }
        }
      },
      "textOnly": {
        "title": "Text",
        "type": "string"
      }
    }
  },
  "styleSheet": {
    "images": {
      "whiteRectImage": {
        "data": "data:image/png;base64,",
        "imageType": "dataUrl"
      },
      "sign": {
        "data": "data:image/svg+xml;base64,",
        "imageType": "dataUrl"
      },
      "smile": {
        "data": "https://openmoji.org/data/color/svg/1F600.svg",
        "imageType": "url"
      },
      "arrow_right": {
        "data": "https://openmoji.org/data/color/svg/27A1.svg",
        "imageType": "url"
      },
      "minus": {
        "data": "https://openmoji.org/data/color/svg/2796.svg",
        "imageType": "url"
      }
    },
    "nodes" : {
      "classes" : {
        "white_rect": {
          "shapeType": "roundrectangle",
          "fill": {
            "color": "#FFF"
          },
          "borderStyle": {
            "color": "#AAA",
            "width": 2.0
          }
        },
        "sign": {
          "image": "sign"
        },
        "smile": {
          "image": "smile"
        }
      }
    },
    "edges": {
      "classes":  {
        "default": {
          "arrows": {
            "source": "circle",
            "target": "standard"
          }
        }
      }
    }
  }
}